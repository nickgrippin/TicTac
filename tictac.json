{
  "id": "tictac",
  "mem": 512,
  "cpus": 0.1,
  "instances": 1,
  "uris": [
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "registry:5000/tictac:{{DOCKER_IMAGE_TAG}}",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8085,
          "hostPort": 0,
          "servicePort": 10085,
          "protocol": "tcp"
        }
      ]
    }
  },
  "labels": {
    "HAPROXY_GROUP": "internal"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/start",
      "gracePeriod": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
